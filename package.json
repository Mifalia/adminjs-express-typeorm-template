{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src\"",
    "start": "npm run build && concurrently \"tsc -w\" \"nodemon ./dist/app.js\"",
    "typeorm": "dotenv -c '.env' -- typeorm -d \"./dist/db/datasource.js\"",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "migration:rollback": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/typeorm": "^5.0.0",
    "adminjs": "^7.4.1",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "mysql2": "latest",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "name": "backoffice"
}
